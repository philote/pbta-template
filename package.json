{
  "name": "pbta-boilerplate",
  "version": "2.0.0",
  "description": "CSS compiler & linting for the PbtA-Boilerplate system",
  "scripts": {
  "build": "sass src/scss/pbta-template.scss css/pbta-template.css --style=expanded --no-source-map",
    "watch": "sass src/scss/pbta-template.scss css/pbta-template.css --style=expanded --source-map --watch",
    "pushLDBtoJSON": "node ./tools/pushLDBtoJSON.mjs",
    "pullJSONtoLDB": "node ./tools/pullJSONtoLDB.mjs",
    "createSymlinks": "node ./tools/create-symlinks.mjs",
    "generate": "node src/generate-pbta-template-module.mjs"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "author": "Joseph Hopson",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@foundryvtt/foundryvtt-cli": "^1.0.3",
    "sass": "^1.53.0",

    "glob": "^10.3.10",
    "inquirer": "^9.2.15",
    "prompt": "^1.3.0",
    "renamer": "^5.0.0",
    "replace": "^1.2.2"
  }
}
